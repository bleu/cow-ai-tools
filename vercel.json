{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functions": {
    "api/index.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "rewrites": [
    { "source": "/api/up", "destination": "/api?path=up" },
    { "source": "/api/predict", "destination": "/api?path=predict" }
  ]
}
